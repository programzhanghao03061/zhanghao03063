.header {
  width: 100%;
  height: 400rpx;
  background: radial-gradient(#fa5f4b, #eb3d3c);
}

.orderName {
  height: 30px;
  width: 100%;
  line-height: 30px;
  padding-left: 20px;
}

.spaceLine {
  height: 1px;
  width: 100%;
  background: #f3f3f3;
}

.spaceBigLine {
  height: 5px;
  width: 100%;
  background: #f3f3f3;
}


.avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
  border: 3px solid #66bbe9;
}

.nickname {
  position: absolute;
  left: 20rpx;
  top: 20rpx;
  color: white;
}

.level {
  color: white;
  padding: 5rpx 40rpx;
  border-radius: 20rpx;
  margin-top: -20rpx;
}

.exp {
  margin-top: 5rpx;
  padding: 5rpx 80rpx;
}

.content {
  padding: 20rpx 0 20rpx 20rpx;
  border-bottom: 1px solid #f3f3f3;
}

.sm-img {
  margin-right: 10rpx;
}

.user-menu {
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #fff;
}

.itemOrder {
  float: left;
  width: 25%;
  height: 187.5rpx;
  text-align: center;
}

.user-menu .item {
  float: left;
  width: 33.33333%;
  height: 187.5rpx;
  border-right: 1px solid rgba(0, 0, 0, 0.15);
  border-bottom: 1px solid rgba(0, 0, 0, 0.15);
  text-align: center;
}

.user-menu .item .a {
  display: flex;
  width: 100%;
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.nav_right_items image {
  /*被图片设置宽高*/
  width: 50px;
  height: 50px;
}

.nav_right_items text {
  /*给text设成块级元素*/
  display: block;
  font-size: 12px;
  /*设置文字溢出部分为...*/
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.user-menu .item.no-border {
  border-right: 0;
}

.user-menu .item.item-bottom {
  border-bottom: none;
}
.nav_right_items {
  /*浮动向左*/
  float: left;
  /*每个item设置宽度是33.33%*/
  justify-content: space-between;
  width: 25%;
  text-align: center;
}
.button {
  width: 100%;
  height: 80px;
 
  /* flex-wrap：wrap ; */
  display: flex;
  justify-content: space-around;
  

  flex-wrap: wrap;
}


